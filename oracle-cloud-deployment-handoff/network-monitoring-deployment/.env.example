# Network Automation Dashboard - Environment Configuration
# Copy this file to .env and update with your values
# NEVER commit the .env file to version control!

# ===== FLASK CONFIGURATION =====
SECRET_KEY=your-secret-key-here-change-this-in-production
FLASK_DEBUG=True
FLASK_HOST=127.0.0.1
FLASK_PORT=5000

# ===== DATABASE CONFIGURATION =====
DATABASE_PATH=data/network_dashboard.db

# ===== NETWORK SETTINGS =====
SNMP_COMMUNITY=public
SSH_TIMEOUT=30
CONNECTION_TIMEOUT=10

# ===== SECURITY SETTINGS =====
ENABLE_SECURITY_SCANNING=True

# ===== PRIVATE DEVICE CREDENTIALS =====
# Add your private network device credentials here
# PRIVATE_DEVICE_1_HOST=192.168.1.1
# PRIVATE_DEVICE_1_USER=admin
# PRIVATE_DEVICE_1_PASS=your_password